#include <iostream>
using namespace std;

int main() {
    int n, sum = 0, temp;
    cout << "Enter a number: ";
    cin >> n;
    temp = n;

    while (n > 0) {
        sum += n % 10;
        n /= 10;
    }

    if (temp % sum == 0)
        cout << temp << " is a Harshad number.";
    else
        cout << temp << " is not a Harshad number.";

    return 0;
}
