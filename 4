#include <iostream>
using namespace std;

int main() {
    cout << "Even numbers (excluding multiples of 4): ";
    for (int i = 1; i <= 20; i++) {
        if (i % 2 != 0) continue;   // Skip odd numbers
        if (i % 4 == 0) continue;   // Skip multiples of 4
        cout << i << " ";
    }
    return 0;
}
