#include <iostream>
using namespace std;

int factorial(int n) {
    int fact = 1;
    for (int i = 1; i <= n; i++)
        fact *= i;
    return fact;
}

int main() {
    cout << "Strong numbers between 1 and 500 are: ";
    for (int n = 1; n <= 500; n++) {
        int num = n, sum = 0;
        while (num > 0) {
            int rem = num % 10;
            sum += factorial(rem);
            num /= 10;
        }
        if (sum == n)
            cout << n << " ";
    }
    return 0;
}
