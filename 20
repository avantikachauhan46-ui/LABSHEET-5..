#include <iostream>
using namespace std;

int main() {
    int n, rev = 0, temp;
    cout << "Enter a number: ";
    cin >> n;
    temp = n;

    while (n > 0) {
        rev = rev * 10 + (n % 10);
        n /= 10;
    }

    cout << "Reversed number: " << rev << endl;
    if (rev == temp)
        cout << "It is a palindrome.";
    else
        cout << "Not a palindrome.";
    return 0;
}
