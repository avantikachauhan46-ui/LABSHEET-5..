#include <iostream>
using namespace std;

int main() {
    int arr[5] = {3, 7, 9, 12, 15};
    int num;
    bool found = false;

    cout << "Enter a number to search: ";
    cin >> num;
    for (int i = 0; i < 5; i++) {
        if (arr[i] == num) {
            cout << "Number found at position " << i + 1 << endl;
            found = true;
            break;
        }
    }

    if (!found)
        cout << "Number not found in the sequence.";
    
    return 0;
}
