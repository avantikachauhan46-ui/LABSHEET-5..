#include <iostream>
using namespace std;

int factorial(int n) {
    int fact = 1;
    for (int i = 1; i <= n; i++)
        fact *= i;
    return fact;
}

int main() {
    int n, temp, sum = 0;
    cout << "Enter a number: ";
    cin >> n;
    temp = n;

    while (n > 0) {
        int rem = n % 10;
        sum += factorial(rem);
        n /= 10;
    }

    if (sum == temp)
        cout << temp << " is a Strong number.";
    else
        cout << temp << " is not a Strong number.";

    return 0;
}
